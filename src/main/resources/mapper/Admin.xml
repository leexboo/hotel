<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leexboo.hotel.mapper.AdminMapper">
    <select id="getAdmin" resultType="com.leexboo.hotel.model.Admin">
        select * from admin where admim_id =#{admin_id} and password = #{password}
    </select>
</mapper>
